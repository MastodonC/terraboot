                "template_file.master-user-data": {
                    "type": "template_file",
                    "depends_on": [
                        "aws_elb.InternalMasterLoadBalancer",
                        "aws_iam_access_key.host-key",
                        "aws_iam_access_key.host-key",
                        "aws_iam_role.master-role",
                        "aws_iam_role.slave-role"
                    ],
                    "primary": {
                        "id": "f0fd2fe7c5369a06282069ec44d769646a62efe83a0b25222f95d7fc779af592",
                        "attributes": {
